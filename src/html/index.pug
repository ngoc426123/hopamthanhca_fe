        .comp-box.--transparent
          .comp-box__title
            h3.comp-box__title-text Xem nhiều trong tháng
          .comp-box__content.--background.--padding
            include _components/_home-ironsong.pug
      .col-12.col-md-12.col-lg-4
        .comp-box.--transparent
          .comp-box__title
            h3.comp-box__title-text Yêu thích nhất trang
          .comp-box__content.--background.--padding
            include _components/_home-ironsong.pug
    include _components/_home-author.pug
