extends _layout/_layout

block main-content
  .comp-wrapper
    include _components/_home-season.pug
    .row.justify-content-center
      .col-lg-6
        include _components/_home-newsong.pug
      .col-lg-6 
        include _components/_home-mostview.pug
    include _components/_home-category.pug
    .row
      .col-12.col-md-6.col-lg-4
        .comp-box.--transparent
          .comp-box__title
            h3.comp-box__title-text Xem nhiều trong tuần
          .comp-box__content.--background.--padding
            include _components/_home-ironsong.pug
